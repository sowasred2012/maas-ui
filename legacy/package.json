{
  "name": "@maas-ui/maas-ui-legacy",
  "version": "1.3.1",
  "license": "AGPL-3.0",
  "private": true,
  "main": "./dist/main.js",
  "files": [
    "dist/**/*.js",
    "!/dist/**/*.test.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/canonical-web-and-design/maas-ui.git"
  },
  "bugs": "https://github.com/canonical-web-and-design/maas-ui/issues",
  "scripts": {
    "build": "NODE_ENV=production webpack --progress --config webpack.prod.js",
    "build-dev": "NODE_ENV=development webpack --config webpack.dev.js",
    "clean": "rm -rf node_modules dist",
    "clean-build": "rm -rf dist",
    "lint": "yarn lint-package-json && yarn lint-js",
    "lint-js": "eslint ./src/app/",
    "lint-package-json": "npmPkgJsonLint .",
    "prettier": "prettier --write 'src/**/*.{js,scss}' '!**/build.scss' '!**/*-min.js'",
    "prettier-check": "prettier --check 'src/**/*.{js,scss}' '!**/build.scss' '!**/*-min.js'",
    "prepublishOnly": "yarn build",
    "start": "NODE_ENV=development webpack serve --config webpack.dev.js --port 8402 --host 0.0.0.0 --disable-host-check",
    "test": "jest",
    "watch": "NODE_ENV=development webpack --watch"
  },
  "devDependencies": {
    "@babel/cli": "7.14.3",
    "@babel/core": "7.14.3",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/preset-env": "7.14.4",
    "@babel/preset-react": "7.13.13",
    "@types/prop-types": "15.7.3",
    "angular-mocks": "1.8.2",
    "autoprefixer": "10.2.6",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "babel-plugin-angularjs-annotate": "0.10.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "8.1.1",
    "core-js": "3.12.1",
    "css-loader": "5.2.6",
    "css-minimizer-webpack-plugin": "2.0.0",
    "dotenv-flow-webpack": "1.1.0",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.6.2",
    "eslint": "7.27.0",
    "eslint-plugin-react": "7.23.2",
    "html-loader-jest": "0.2.1",
    "html-webpack-plugin": "5.3.1",
    "jest": "26.6.3",
    "jest-fetch-mock": "3.0.3",
    "mini-css-extract-plugin": "1.6.0",
    "npm-package-json-lint": "5.1.0",
    "postcss": "8.3.0",
    "postcss-loader": "5.3.0",
    "prettier": "2.3.0",
    "process": "0.11.10",
    "prop-types": "15.7.2",
    "raw-loader": "4.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "1.34.0",
    "sass-loader": "11.1.1",
    "webpack": "5.37.1",
    "webpack-cli": "4.7.0",
    "webpack-dev-server": "3.11.2",
    "webpack-merge": "5.7.3"
  },
  "dependencies": {
    "@maas-ui/maas-ui-shared": "1.3.1",
    "@sentry/browser": "6.4.1",
    "@sentry/integrations": "6.4.1",
    "@uirouter/angularjs": "1.0.29",
    "angular": "1.8.2",
    "angular-cookies": "1.8.2",
    "angular-route": "1.8.2",
    "angular-sanitize": "1.8.2",
    "angular-vs-repeat": "1.1.11",
    "cross-env": "7.0.3",
    "jquery": "3.6.0",
    "js-cookie": "2.2.1",
    "ng-tags-input": "3.2.0",
    "single-spa-angularjs": "4.2.3",
    "vanilla-framework": "2.30.0"
  },
  "npmpackagejsonlint": {
    "rules": {
      "prefer-absolute-version-dependencies": "error",
      "prefer-absolute-version-devDependencies": "error",
      "prefer-alphabetical-dependencies": "error",
      "prefer-alphabetical-devDependencies": "error"
    }
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "lodash": "4.17.21"
  }
}
